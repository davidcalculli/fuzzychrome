{
   "manifest_version": 2,
   "name": "Fuzzychrome",
   "description": "Fuzzychrome",
   "version": "0.1",
   "icons": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
   },
   "background": {
      "scripts": [
         "src/lib/ayepromise.js",
         "src/lib/lodash.min.js",
         "src/lib/zepto.min.js",
         "src/shared.js",
         "src/background.js"
      ]
   },
   "browser_action": {
      "default_icon": "assets/icon24.png",
      "default_popup": "settings.html"
   },
   "permissions": [
      "tabs"
   ],
   "commands": {
      "tab-mode": {
         "suggested_key": {
            "default": "Ctrl+Shift+Down",
            "windows": "Ctrl+Shift+Down",
            "mac": "Command+Shift+Down"
         },
         "description": "Tab Mode"
      },
      "overlay-mode": {
         "suggested_key": {
            "default": "Ctrl+Shift+Down",
            "windows": "Ctrl+Shift+Down",
            "mac": "Command+Shift+Down"
         },
         "description": "Overlay Mode"
      }
   }
}
